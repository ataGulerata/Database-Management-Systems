        <div class="page-wrapper">
            <div class="content">
                <div class="row">
                    <div class="col-lg-8 offset-lg-2">
                        <h4 class="page-title">Doktor Sil</h4>
                    </div>
                </div>
                <div class="row">

                    <div class="col-lg-8 offset-lg-2">
                        <form method="post">
                            <div class="row">
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <% for (var i = 0; i < list.length; i++){ %>
                                        <label>Doktor Adı <span class="text-danger">*</span></label>
                                        <input class="form-control" type="text" name="first_name" value="<%= list[i].first_name %>">
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label>Soyadı</label>
                                        <input class="form-control" type="text" name="last_name" value="<%= list[i].last_name %>">
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                   
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label>Email <span class="text-danger">*</span></label>
                                        <input class="form-control" type="email" name="email" value="<%= list[i].email %>">
                                    </div>
                                </div>
                               
                                
								<div class="col-sm-6">
                                    <div class="form-group">
                                        <label>Doğum Tarihi</label>
                                        <div class="cal-icon">
                                            <input type="text" class="form-control datetimepicker" name="dob" value="<%= list[i].dob %>">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-6">
									<div class="form-group gender-select">
										<label class="gen-label">Cinsiyet:</label>
										<div class="form-check-inline">
											<label class="form-check-label">
												<input type="radio" name="gender" class="form-check-input" value="male" >Erkek
											</label>
										</div>
										<div class="form-check-inline">
											<label class="form-check-label">
												<input type="radio" name="gender" class="form-check-input" value="female">Kadın
											</label>
										</div>
									</div>
                                </div>
								<div class="col-sm-12">
									<div class="row">
										<div class="col-sm-12">
											<div class="form-group">
												<label>Addres</label>
												<input type="text" class="form-control " name="address" value="<%= list[i].address %>">
											</div>
										</div>
										
										
										
										
									</div>
								</div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label>Telefon Numarası </label>
                                        <input class="form-control" type="text" name="phone" value="<%= list[i].phone %>">
                                    </div>
                                </div>
                                <div class="col-sm-6">
									<div class="form-group">
										<label>Avatar</label>
										<div class="profile-upload">
											<div class="upload-img">
												<img alt="" src="/assets/img/user.jpg">
											</div>
											<div class="upload-input">
												<input type="file" class="form-control" name="image" value="<%= list[i].image %>">
											</div>
										</div>
									</div>
                                </div>

                                <div class="col-sm-6 col-md-6 col-lg-3">
                                    <div class="form-group">
                                        <label>Department</label>
                                        <select class="form-control select" name="department" value="<%= list[i].department %>">
                                            <option value="Neurology">Nöroloji</option>
                                            <option value="Opthalmology">Ortopedi</option>
                                            <option value="Orthopedics">Kalp hastalıkları</option>
                                            <option value="Cancer Department" >kanser Hastalıkları</option>
                                            <option value="ENT Department " >ENT</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
							<div class="form-group">
                                <label>Kısa Özgeçmiş</label>
                                <textarea class="form-control" rows="3" cols="30" name="biography" value="<%= list[i].biography %>"></textarea>
                            </div>

                            <% } %>
                   
                            <div class="m-t-20 text-center">
                                <button class="btn btn-primary submit-btn">Doktor Sil</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
