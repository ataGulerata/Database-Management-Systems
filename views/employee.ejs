        <div class="page-wrapper">
            <div class="content">
                <div class="row">
                    <div class="col-sm-4 col-3">
                        <h4 class="page-title">Çalışanlar</h4>
                    </div>
                    <div class="col-sm-8 col-9 text-right m-b-20">
                        <a href="/employee/add" class="btn btn-primary float-right btn-rounded"><i class="fa fa-plus"></i> Çalışan Ekle</a>
                    </div>
                </div>
                <form action="" method="POST">
                <div class="row filter-row">
                   
                    <div class="col-sm-6 col-md-3">
                        <div class="form-group form-focus">
                            <label class="focus-label">Çalışan Adı / Numarası </label>
                            <input type="text" class="form-control floating" name="search">
                        </div>
                    </div>
                   
                    <div class="col-sm-6 col-md-3">
                       <button formaction="/employee/search" class="btn btn-success btn-block">Ara</button>
                    </div>
                </div>
                </form>
            
                <div class="row">
                    <div class="col-md-12">
						<div class="table-responsive">
                            <table class="table table-striped custom-table">
                                <thead>
                                    <tr>
                                        <th style="min-width:100px;">Numara</th>
                                        <th>Çalışan Adı</th>
                                        <th>Email</th>
                                        <th>Telefon Numarası</th>
                                        <th style="min-width: 110px;">Başlama Tarihi</th>
                                        <th>Görev</th>
                                        <th class="text-right">Eylem</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% for (var i = 0; i < employee.length; i++){ %>
									
                                        
									<tr>
                                        <td>
											<%= employee[i].id %>
										</td>
                                        <td><%= employee[i].name %></td>
                                        <td><%= employee[i].email %></td>
                                        <td><%= employee[i].contact %></td>
                                        <td><%= employee[i].join_date %></td>
                                        <td>
                                            <%= employee[i].role %>
                                        </td>
                                        <td class="text-right">
                                            <div class="dropdown dropdown-action">
                                                <a href="#" class="action-icon dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><i class="fa fa-ellipsis-v"></i></a>
                                                <div class="dropdown-menu dropdown-menu-right">
                                                    <a class="dropdown-item" href="/employee/edit_employee/<%= employee[i].id %>"><i class="fa fa-pencil m-r-5"></i> Edit</a>
                                                    <a class="dropdown-item" href="/employee/delete_employee/<%= employee[i].id %>" ><i class="fa fa-trash-o m-r-5"></i> Delete</a>
                                                </div>
                                            </div>
                                        </td>
                                        <% } %>
                                    </tr>
                                </tbody>
                            </table>
						</div>
                    </div>
                </div>
            </div>
